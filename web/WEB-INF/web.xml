<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
     </session-config>   
        <servlet>
            <servlet-name>menu</servlet-name>
            <servlet-class>Servlet.Menu</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>menu</servlet-name>
            <url-pattern>/servlet/Menu</url-pattern>
        </servlet-mapping>
        <servlet>
            <servlet-name>update</servlet-name>
            <servlet-class>Servlet.UpdateData</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>update</servlet-name>
            <url-pattern>/servlet/Update</url-pattern>
        </servlet-mapping>
        <servlet>
            <servlet-name>reg</servlet-name>
            <servlet-class>Servlet.Registration</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>reg</servlet-name>
            <url-pattern>/servlet/Reg</url-pattern>
        </servlet-mapping>
        <servlet>
            <servlet-name>xyz</servlet-name>
            <servlet-class>Servlet.Servlet1</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>xyz</servlet-name>
            <url-pattern>/servlet/abc</url-pattern>
        </servlet-mapping>
        
        <servlet>
            <servlet-name>rep</servlet-name>
            <servlet-class>Servlet.RegistrationReport</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>rep</servlet-name>
            <url-pattern>/servlet/Rep</url-pattern>
        </servlet-mapping>
        <servlet>
            <servlet-name>kaf</servlet-name>
            <servlet-class>Servlet.Login</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>kaf</servlet-name>
            <url-pattern>/servlet/pkf</url-pattern>
        </servlet-mapping>
        <servlet>
            <servlet-name>Log</servlet-name>
            <servlet-class>Servlet.LogOf</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>Log</servlet-name>
            <url-pattern>/servlet/Log</url-pattern>
        </servlet-mapping>
        
        <servlet>
            <servlet-name>del</servlet-name>
            <servlet-class>Servlet.DeleteReport</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>del</servlet-name>
            <url-pattern>/servlet/Del</url-pattern>
        </servlet-mapping>
        <servlet>
            <servlet-name>delrep</servlet-name>
            <servlet-class>Servlet.DelRep</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>delrep</servlet-name>
            <url-pattern>/servlet/DelReport</url-pattern>
        </servlet-mapping>
        <servlet>
            <servlet-name>rupdate</servlet-name>
            <servlet-class>Servlet.ReportUpdate</servlet-class>
        </servlet> 
        <servlet-mapping>
            <servlet-name>rupdate</servlet-name>
            <url-pattern>/servlet/Rupdate</url-pattern>
        </servlet-mapping>   
        <servlet>
            <servlet-name>upd</servlet-name>
            <servlet-class>Servlet.UpdateDetails</servlet-class>
        </servlet> 
        <servlet-mapping>
            <servlet-name>upd</servlet-name>
            <url-pattern>/servlet/Upd</url-pattern>
        </servlet-mapping> 
    <welcome-file-list>
        <welcome-file>Login.html</welcome-file>
    </welcome-file-list>
</web-app>
